{
    "name": "seydu/eloquent-metadata",
    "type": "library",
    "description": "Manage metadata for Eloquent models to make automation easier",
    "keywords": [
        "seydu",
        "eloquent-metadata"
    ],
    "homepage": "https://github.com/seydu/eloquent-metadata",
    "license": "MIT",
    "authors": [
        {
            "name": "Saidou Gueye",
            "email": "saidou.gueye@gmail.com",
            "homepage": "https://github.com/seydu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "psr/cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Seydu\\EloquentMetadata\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seydu\\EloquentMetadata\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
